<h1>Listing twitter_accessors</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @twitter_accessors.each do |twitter_accessor| %>
      <tr>
        <td><%= link_to 'Show', twitter_accessor %></td>
        <td><%= link_to 'Edit', edit_twitter_accessor_path(twitter_accessor) %></td>
        <td><%= link_to 'Destroy', twitter_accessor, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Twitter accessor', new_twitter_accessor_path %>
